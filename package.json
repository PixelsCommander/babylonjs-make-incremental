  "license": "Apache 2.0",
  "repository": "https://github.com/evolution-gaming/babylonjs-make-incremental",
  "keywords": [
    "babylonjs",
    "incremental"
  ],
  "devDependencies": {
    "@types/node": "^8.0.14",
    "tslint": "^5.5.0",
    "tslint-eslint-rules": "3.4.0",
    "typescript": "^2.4.1"
  }
}
